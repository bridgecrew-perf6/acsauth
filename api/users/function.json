{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ],
      "route": "users/{email}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "name": "getUser",
      "databaseName": "ACS",
      "collectionName": "users",
      "connectionStringSetting": "MyAccount_COSMOSDB",
      "id" : "{email}",
      "partitionKey": "{email}",
      "direction": "in"
    }
  ],
  "scriptFile": "../dist/users/index.js"
}
